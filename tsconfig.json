{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "allowJs": true,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "types": [
      "node",
      "astro/client"
    ],
    "paths": {
      "@/*": ["./src/*"],
    },
    "jsxImportSource": "react"
  }
}